	file = {Teixeira et al. - 2023 - Flexitarians in Brazil Who are They, What do They.pdf:C\:\\Users\\claudia.rodes\\Zotero\\storage\\WFL4EH6Y\\Teixeira et al. - 2023 - Flexitarians in Brazil Who are They, What do They.pdf:application/pdf},
