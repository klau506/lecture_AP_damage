	file = {Kemp et al. - 2022 - Climate Endgame Exploring catastrophic climate ch.pdf:C\:\\Users\\claudia.rodes\\Zotero\\storage\\FY86PPBD\\Kemp et al. - 2022 - Climate Endgame Exploring catastrophic climate ch.pdf:application/pdf},
