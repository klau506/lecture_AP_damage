	urldate = {2023-12-12},
	journal = {Nature Communications},
	author = {Harmsen, Mathijs and Tabak, Charlotte and Höglund-Isaksson, Lena and Humpenöder, Florian and Purohit, Pallav and Van Vuuren, Detlef},
	month = jun,
