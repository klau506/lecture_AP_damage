	journal = {BMC Medicine},
	author = {Aune, Dagfinn and Keum, NaNa and Giovannucci, Edward and Fadnes, Lars T. and Boffetta, Paolo and Greenwood, Darren C. and Tonstad, Serena and Vatten, Lars J. and Riboli, Elio and Norat, Teresa},
